<UserControl x:Class="UV_Eats_Client.Client.UserControls.CategoriaProducto"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:UV_Eats_Client.Client.UserControls"
             mc:Ignorable="d" Height="91" Width="90">
    <Grid Background="#FFE87A7A" Margin="14,14,14,14">
        <Image x:Name="imagenCategoria" Margin="15,10,16,29" Source="/Client/Resources/dislike.png"/>
        <Label x:Name="nombreCategoria" Content="Label" Margin="3,39,0,0" HorizontalAlignment="Left" Width="50" FontSize="9"/>
        <Button Content="" HorizontalAlignment="Center" VerticalAlignment="Center" Height="64" Cursor="Hand" Width="62" Click="cambiarCategoria" Background="{x:Null}"/>
    </Grid>
</UserControl>
